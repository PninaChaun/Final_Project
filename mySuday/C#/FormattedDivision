using System;

class MainClass {

  public static string FormattedDivision(int num1, int num2) {

    decimal n = (decimal)num1 / (decimal)num2;

    return n.ToString("#,###0.####");

  }

  static void Main() {  
    // keep this function call here
    Console.WriteLine(FormattedDivision(Console.ReadLine()));
  } 

}