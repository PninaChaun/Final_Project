using System;

class MainClass {

  public static string PrimeTime(int num) {

    for (int i = 2; i < num; i++)
    {
      if (num % i == 0)
      {
        return "false";
      }
    }

    return "true";

  }

  static void Main() {  
    // keep this function call here
    Console.WriteLine(PrimeTime(Console.ReadLine()));
  } 

}